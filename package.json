{
  "name": "papelera_naty_backend",
  "version": "0.1.0",
  "description": "Papelera Naty Backend",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_PATH=. NODE_ENV=development node server",
    "test": "cross-env NODE_PATH=. NODE_ENV=test nyc --reporter=lcov mocha  --exit -u exports -R spec configs/testsconfig",
    "singletest": "cross-env NODE_PATH=. NODE_ENV=test nyc --reporter=html mocha --exit --recursive -R spec $TESTTORUN"
  },
  "author": "Maximiliano Carrizo",
  "_moduleAliases": {
    "@appconfig": "configs/appconfig",
    "@logger": "configs/loggerconfig",
    "@ormconfig": "configs/ormconfig",
    "@routesconfig": "configs/routesconfig",
    "@expressconfig": "configs/expressconfig",
    "@errorhandlerconfig": "configs/errorhandlerconfig",
    "@migrationconfig": "configs/migrationconfig",
    "@modelsutil": "utils/modelsutil",
    "@validationutil": "utils/validationutil",
    "@modelsqueries": "app/models/modelsqueries"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dateformat": "^3.0.3",
    "dotenv-flow": "^0.2.0",
    "exception-formatter": "^1.0.7",
    "expect": "^23.6.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "liquibase": "0.0.3",
    "memory-cache": "^0.2.0",
    "mocha": "^5.2.0",
    "module-alias": "^2.1.0",
    "nyc": "^13.1.0",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "promise.prototype.finally": "^3.1.0",
    "sequelize": "^4.42.0",
    "sequelize-log-syntax-colors": "^2.0.0",
    "supertest": "^3.3.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0"
  }
}
